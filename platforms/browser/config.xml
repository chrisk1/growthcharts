<?xml version='1.0' encoding='utf-8'?>
<widget id="com.incubate.growthcharts" version="1.1.0" versionCode="19" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <gap:config-file parent="CFBundleShortVersionString" platform="ios">
        <string>7</string>
    </gap:config-file>
    <name>Growth Charts</name>
    <description>
        Calculate growth chart centiles for children using UK-WHO data from the Royal College of Paediatrics and Child Health.
    </description>
    <author email="hello@paediatrics.co.uk" href="http://www.paediatrics.co.uk">
        Christopher Kelly 
    </author>
    <access browserOnly="true" origin="http://www.paediatrics.co.uk" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="mdpi" height="57" platform="ios" src="icon-57.png" width="57" />
    <gap:splash src="splash.png" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-splashscreen" source="npm" />
    <gap:plugin name="cordova-plugin-inappbrowser" source="npm" />
    <gap:plugin name="cordova-plugin-dialogs" source="npm" />
    <gap:plugin name="flyer299-cordova-plugin-google-analytics" source="npm" />
    <gap:plugin name="cordova-plugin-globalization" source="npm" />
    <gap:plugin name="cordova-plugin-apprate" source="npm" />
    <gap:plugin name="cordova-plugin-device" source="npm" />
    <gap:plugin name="cordova-plugin-wkwebview-engine" source="npm" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="stay-in-webview" value="false" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="100000" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="blackopaque" />
    <preference name="target-device" value="handset" />
    <preference name="permissions" value="none" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="android-minSdkVersion" value="17" />
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
</widget>
